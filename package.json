{
    "name": "compass-car",
    "version": "1.0.0",
    "main": "src/main.ts",
    "type": "module",
    "scripts": {
        "start": "cross-env NODE_ENV=production node .build/main.js",
        "typeorm": "typeorm-ts-node-esm",
        "migration:generate": "typeorm-ts-node-esm -d ./src/data/data-source.ts migration:generate",
        "migration:show": "typeorm-ts-node-esm -d ./src/data/data-source.ts migration:show",
        "migration": "typeorm-ts-node-esm migration:run -d ./src/data/data-source.ts",
        "test": "vitest --ui --open false --no-file-parallelism --testTimeout 600000",
        "dev": "nodemon -w ./ -e ts,json -x \"npm run build && cross-env NODE_ENV=development node --inspect=0.0.0.0:9229 .build/main.js\"",
        "build": "tsc",
        "script": "npm run build && node .build/script.js",
        "lint": "eslint src",
        "ts-check": "tsc"
    },
    "license": "ISC",
    "description": "This is a activity of CompassOUL nodejs learning path",
    "dependencies": {
        "argon2": "^0.41.1",
        "axios": "^1.7.7",
        "compass-car": "file:",
        "cors": "^2.8.5",
        "deepmerge": "^4.3.1",
        "express": "^4.21.0",
        "helmet": "^8.0.0",
        "jsonwebtoken": "^9.0.2",
        "libphonenumber-js": "^1.11.12",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "sqlite3": "^5.1.7",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.2",
        "typeorm": "^0.3.20",
        "validator-brazil": "^1.2.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@types/argon2": "^0.14.1",
        "@types/cors": "^2.8.17",
        "@types/deepmerge": "^2.1.0",
        "@types/dotenv": "^6.1.1",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^5.0.0",
        "@types/glob": "^8.1.0",
        "@types/helmet": "^0.0.48",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.7.4",
        "@types/swagger-ui-express": "^4.1.7",
        "@vitest/ui": "^2.1.3",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "esbuild": "^0.24.0",
        "eslint": "^9.11.1",
        "globals": "^15.9.0",
        "morgan": "^1.10.0",
        "nodemon": "^3.1.7",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.8.0",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.1.3"
    }
}
